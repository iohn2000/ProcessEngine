    <script src="@Url.Content("~/Scripts/kendo/2017.3.913/cultures/kendo.culture.en-US.min.js")"></script>

    <script type="text/javascript">
        kendo.culture("en-US");
        var myculture = kendo.culture();

        //Override date-formats for every culture, http://docs.telerik.com/kendo-ui/framework/globalization/dateformatting
        kendo.culture().calendar.patterns.d = "dd.MM.yyyy"; //short date
        kendo.culture().calendar.patterns.D = "dddd, dd. MMMM yyyy"; //long date
        kendo.culture().calendar.patterns.F = "dddd, dd. MMMM yyyy HH:mm:ss"; //full date/time
        kendo.culture().calendar.patterns.g = "dd.MM.yyyy HH:mm"; //general date/time (short time)
        kendo.culture().calendar.patterns.G = "dd.MM.yyyy HH:mm:ss"; //general date/time (long time)
        kendo.culture().calendar.patterns.m = "d. MMMM"; //month/day
        kendo.culture().calendar.patterns.M = "d. MMMM";
        kendo.culture().calendar.patterns.t = "HH:mm"; //short time
        kendo.culture().calendar.patterns.T = "HH:mm:ss"; //long time
        kendo.culture().calendar.patterns.s = "yyyy'-'MM'-'dd'T'HH':'mm':'ss"; //universal sortable local date/time
        kendo.culture().calendar.patterns.u = "yyyy'-'MM'-'dd HH':'mm':'ss'Z'"; //universal sortable UTC date/time
        kendo.culture().calendar.patterns.y = "MMMM yyyy"; //or 'Y', year/month
        kendo.culture().calendar.patterns.Y = "MMMM yyyy";

        console.log('Current Kendo culture: ' + myculture.name);
    </script>
